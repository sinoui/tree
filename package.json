{
  "name": "sinoui-tree",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "homepage": "https://sinoui.github.io/tree",
  "bugs": {
    "url": "https://github.com/sinoui/tree/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinoui/tree.git"
  },
  "scripts": {
    "test": "ts-lib-tools test",
    "build": "lerna run build",
    "format": "ts-lib-tools format",
    "lint": "ts-lib-tools lint",
    "gen": "ts-lib-tools gen-module",
    "postinstall": " yarn-deduplicate yarn.lock",
    "doc:dev": "docz dev",
    "doc:build": "docz build",
    "doc:publish": "docz build && gh-pages -d .docz/dist"
  },
  "prettier": {
    "printWidth": 80,
    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always"
  },
  "eslintConfig": {
    "extends": "ts-lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "packages/**/*.{ts,tsx}": [
      "cross-env CI=true ts-lib-tools test",
      "eslint --fix --color",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@babel/runtime": "^7.9.2",
    "react-dnd": "^3.0.2",
    "react-dnd-html5-backend": "^3.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.0",
    "@testing-library/react": "^9.3.0",
    "@testing-library/react-hooks": "^3.1.1",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "css-loader": "^3.2.0",
    "docz": "^1.3.2",
    "docz-theme-default": "^1.2.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "lerna": "^3.18.3",
    "lint-staged": "^9.4.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-test-renderer": "^16.11.0",
    "style-loader": "^1.0.0",
    "ts-lib-tools": "^0.5.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.3",
    "webpack-docz-ghpages-plugin": "^0.5.0",
    "yarn-deduplicate": "^2.0.0"
  },
  "resolutions": {
    "styled-components": "^5.1.0"
  }
}
